{
  "version": "1.0.57",
  "name": "@jetplane/velocity-tools",
  "description": "Velocity tools",
  "author": "WolfRayet",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jetplane/velocity-tools.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/velocity-tools.cjs",
  "module": "dist/react.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=16.x",
    "npm": ">=8.x"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/velocity-tools.js",
      "require": "./dist/velocity-tools.cjs"
    }
  },
  "babelMacros": {
    "twin": {
      "config": "./tailwind.config.cjs",
      "preset": "styled-components"
    }
  },
  "scripts": {
    "build": "vite build",
    "start": "rollup -c -w",
    "test": "jest",
    "publish-patch": "npm run build && npm version patch && npm publish"
  },
  "peerDependencies": {
    "@meshsdk/core": "1.5.x",
    "@meshsdk/react": "1.1.x",
    "react-dom": "17.x || 18.x",
    "react": "17.x || 18.x"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@rollup/plugin-babel": "6.0.2",
    "@rollup/plugin-typescript": "9.0.2",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-interactions": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/builder-vite": "0.2.5",
    "@storybook/react": "6.5.13",
    "@storybook/testing-library": "0.0.13",
    "@types/styled-components": "5.1.26",
    "@vitejs/plugin-react": "2.2.0",
    "autoprefixer": "10.4.12",
    "babel-loader": "8.2.5",
    "babel-plugin-macros": "3.1.0",
    "babel-plugin-styled-components": "2.0.7",
    "postcss": "8.4.18",
    "rollup": "3.2.3",
    "tailwindcss": "3.2.1",
    "twin.macro": "3.0.0-rc.5",
    "vite": "3.1.4"
  },
  "keywords": [
    "react",
    "react hooks",
    "typescript",
    "npm"
  ],
  "dependencies": {
    "styled-components": "5.3.6",
    "prop-types": "^15.7.2"
  }
}
